{
  "$schema": "https://raw.githubusercontent.com/teleporthq/teleport-code-generators/master/src/uidl-definitions/schemas/project.json",
  "name": "myVueProject",
  "globals": {
    "settings": {
      "language": "en",
      "title": "Router Project"
    },
    "assets": [],
    "meta": []
  },
  "root": {
    "name": "App",
    "stateDefinitions": {
      "router": {
        "type": "router",
        "defaultValue": "home",
        "values": [
          {
            "value": "home",
            "meta": {
              "path": "/",
              "componentName": "HomePage",
              "fileName": "Home"
            }
          },
          {
            "value": "about",
            "meta": {
              "path": "/mask-url",
              "componentName": "AboutPage"
            }
          }
        ]
      }
    },
    "content": {
      "type": "states",
      "name": "router",
      "states": [
        {
          "value": "home",
          "content": {
            "type": "container",
            "name": "container",
            "children": [
              {
                "type": "button",
                "name": "button",
                "children": ["click-me"]
              },
              {
                "type": "CustomText",
                "name": "text",
                "attrs": {
                  "message": "random message"
                },
                "dependency": {
                  "type": "local"
                }
              },
              {
                "type": "navlink",
                "name": "navlink",
                "attrs": {
                  "transitionTo": "/mask-url"
                },
                "children": ["About page"]
              }
            ]
          }
        },
        {
          "value": "about",
          "content": {
            "type": "group",
            "name": "around",
            "children": [
              {
                "type": "text",
                "name": "aspan",
                "children": ["Who"]
              },
              {
                "type": "container",
                "name": "adiv",
                "children": ["are"]
              },
              {
                "type": "text",
                "name": "aspan2",
                "children": ["you?"],
                "attrs": {
                  "disabled": "true",
                  "data-test": "123"
                }
              },
              {
                "type": "Datepicker",
                "style": {
                  "margin": "10px"
                },
                "dependency": {
                  "type": "package",
                  "path": "vuejs-datepicker",
                  "version": "1.5.4",
                  "meta": {
                    "namedImport": false
                  }
                }
              }
            ]
          }
        }
      ]
    }
  },
  "components": {
    "CustomText": {
      "name": "CustomText",
      "propDefinitions": {
        "message": {
          "type": "string",
          "defaultValue": ""
        }
      },
      "content": {
        "type": "container",
        "style": {
          "color": "red"
        },
        "attrs": {
          "data-attr": "random"
        },
        "children": [
          {
            "type": "text",
            "children": ["$props.message"]
          },
          {
            "type": "dropdown",
            "attrs": {
              "options": ["angular", "react", "vue"]
            }
          },
          {
            "type": "list",
            "attrs": {
              "items": ["Hello", "is it me", "you're", "looking", "for?"]
            }
          }
        ]
      }
    },
    "Card": {
      "name": "Card",
      "propDefinitions": {
        "item": {
          "type": "string",
          "defaultValue": ""
        }
      },
      "content": {
        "type": "container",
        "name": "card",
        "style": {
          "margin": "10px",
          "padding": "10px",
          "border": "1px solid black"
        },
        "children": ["$props.item"]
      }
    },
    "Cards": {
      "name": "Cards",
      "propDefinitions": {
        "items": {
          "type": "array",
          "defaultValue": []
        }
      },
      "content": {
        "type": "container",
        "name": "cards-container",
        "style": {
          "width": "100%",
          "border": "1px solid red",
          "display": "flex"
        },
        "children": [{
          "type": "text",
          "name": "joker",
          "children": ["Last"]
        }],
        "repeat": {
          "content": {
            "type": "container",
            "name": "card-container",
            "style": {
              "backgroundColor": "royalblue",
              "color": "white",
              "margin": "10px",
              "border": "1px solid black"
            },
            "children": [{
              "type": "Card",
              "name": "card",
              "attrs": {
                "item": "$local.item",
                "test": "$local.index"
              },
              "dependency": {
                "type": "local"
              }
            }]
          },
          "dataSource": "$props.items"
        }
      }
    }
  }
}